GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/LinqToObjectiveC" "${PODS_ROOT}/Headers/Public/ReactiveCocoa"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/LinqToObjectiveC" -isystem "${PODS_ROOT}/Headers/Public/ReactiveCocoa"
OTHER_LDFLAGS = -ObjC -l"Pods-SampleApp-LinqToObjectiveC" -l"Pods-SampleApp-ReactiveCocoa"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods